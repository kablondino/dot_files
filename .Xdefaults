!! Capitalization is IMPORTANT!
URxvt.background: [90]#000000
!!URxvt.background: rgba:0000/0000/0000/cccc
URxvt.foreground: #cccccc

URxvt.font: xft:DejaVuSansMono:size=16:pixelsize=14
URxvt.intensityStyles: false
!!URxvt*letterSpace: -0.5

!! Extensions
URxvt.perl-ext-common: default,matcher,resize-font,keyboard-select,fullscreen
URxvt.resize-font.smaller: C-j
URxvt.resize-font.bigger: C-k
URxvt.keysym.F11: perl:fullscreen:switch

URxvt.borderLess: false
URxvt.depth: 32

URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.urlLauncher: chromium
URxvt.matcher.button: 1
URxvt.fading: 20

URxvt.scrollBar: false
URxvt.mouseWheelScrollPage: false
!!URxvt.scrollBar_right: true
!!URxvt.scrollStyle: rvxt
!
URxvt.visualBell: true
URxvt.urgentOnBell: true

URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.cursorColor: green
URxvt.cursorColor2: black


