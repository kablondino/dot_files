[diff]
	tool = vimdiff
[merge]
	tool = vimdiff
[color]
	ui = auto
[color "branch"]
	current = yellow
	local = green
	remote = red
[color "diff"]
	meta = cyan
	frag = magenta
	old = red
	new = green
	whitespace = red reverse
[color "status"]
	added = green
	changed = yellow
	untracked = red
[alias]
	logadog = log --all --decorate --oneline --graph
[core]
	pager = less -FR
[pull]
	rebase = false

